            $(document).ready(function () {
              // Abre o modal de ultimas pesquisas
              var campo_busca = document.querySelector('#q');
              var body = document.body;
              var except = document.getElementsByClassName('span_ultimas_buscas');

              // Mostra opcoes ao clicar no campo de busca
              campo_busca.addEventListener('focus', function(){
                  if($(this).val() ==""){
                    //$("#header").append(document.querySelector('#div_mostra_ultimas_buscas'));
                    $("#header").append(document.querySelector('#div_mostra_ultimas_buscas'));
                    //this.parentNode.appendChild(document.querySelector('#div_mostra_ultimas_buscas'));

                    // Verifica se a div estah vazia antes de mostrar.
                    if($('#modal_ultimas_buscas').text().trim().length > 1){
                      $('#div_mostra_ultimas_buscas').show()
                    }
                  }
              });
              // Em qquer lugar que clicar, vai ocultar a div das palavras buscadas,
              // exceto no campo de busca, que mostra.
              body.addEventListener("click", function () {
                  $('#div_mostra_ultimas_buscas').hide()
              }, false);
              campo_busca.addEventListener("click", function (ev) {
                  ev.stopPropagation();
              }, false);

              // Adiciona os eventos as divs das buscas efetuadas.
              // Ao clicar prenche o campo de busca.
              var ubr_elemen = document.getElementsByClassName('span_ultimas_buscas');
              for (var i = 0; i < ubr_elemen.length; i++) {
                    ubr_elemen[i].addEventListener('click', function(ev) {
                      $("#q").val(this.innerHTML)
                      $('#div_mostra_ultimas_buscas').hide()
                  });
              }
            })




